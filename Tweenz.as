weenCounter++				var zTween = ["zTween"+tweenCounter];				var zTween:Tween = new Tween(itemMC, props[i][0], ease, props[i][1], props[i][2], time, true);				zTween.onMotionFinished = function(){					_root.newTweenz.zTweenEnd();					delete _root.newTweenz.zTween;				}			}			clearInterval(proceedTweenInt);		}		var proceedTweenInt = setInterval(proceedTween, delay);	} 		function zTweenChainInit(){		tweenChain = new Array();	}		function zTweenChainPush(zTweenItem){		tweenChain.push( zTweenItem );	}		function zTweenChainStart(tweenChain){		for (var i; i<= tweenChain.length; i++){			zTweenMultiDelay(tweenChain[i]);		}	}			function zTweenEnd(){		//do something	}	}